{
  "name": "nighout-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pre-commit": "next lint --fix && git add -A .",
    "lint:write": "next lint --cache --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.11",
    "@next/font": "13.1.6",
    "aos": "^3.0.0-beta.6",
    "cypress": "^12.5.1",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "form-controller-lite": "^1.0.11",
    "husky": "^8.0.3",
    "next": "13.1.6",
    "next-themes": "^0.2.1",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-page-scroll": "^1.0.6",
    "relay-compiler": "^14.1.0",
    "sharp": "^0.31.3",
    "swiper": "^9.1.0",
    "typescript": "^4.9.5",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-relay": "^1.8.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "mrm": "2",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6"
  }
}
